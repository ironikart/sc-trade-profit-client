{
    "parser": "babel-eslint",
    "plugins": ["react"],
    "env": {
        "node": true,
        "browser": true,
        "jquery": true,
        "es6": true
    },
    "globals": {
        "Modernizr": true
    },
    "rules": {
        "react/jsx-uses-vars": 2,
        "block-scoped-var": 2,
        "complexity": [
            1,
            5
        ],
        "camelcase": 2,
        "comma-style": [
            1,
            "last"
        ],
        "consistent-return": 1,
        "curly": [
            2,
            "all"
        ],
        "dot-notation": [
            2,
            {
                "allowKeywords": true
            }
        ],
        "dot-location": [
            2,
            "property"
        ],
        "eqeqeq": [
            2,
            "allow-null"
        ],
        "global-strict": 0,
        "guard-for-in": 2,
        "indent": [
            2,
            4
        ],
        "key-spacing": [
            2,
            {
                "align": "value",
                "beforeColon": false,
                "afterColon": true
            }
        ],
        "max-statements": [
            1,
            15
        ],
        "max-depth": [
            1,
            2
        ],
        "max-len": [
            1,
            120
        ],
        "new-cap": 2,
        "no-bitwise": 2,
        "no-caller": 2,
        "no-cond-assign": 2,
        "no-constant-condition": 2,
        "no-debugger": 2,
        "no-empty": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-parens": 0,
        "no-irregular-whitespace": 2,
        "no-iterator": 2,
        "no-implied-eval": 2,
        "no-invalid-regexp": 2,
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-multi-spaces": 2,
        "no-new": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-plusplus": 2,
        "no-proto": 2,
        "no-script-url": 2,
        "no-sequences": 2,
        "no-shadow": 2,
        "no-undef": 2,
        "no-underscore-dangle": 0,
        "no-unreachable": 2,
        "no-unused-vars": [2, {"varsIgnorePattern": "^React$"}],
        "no-use-before-define": 2,
        "no-with": 2,
        "use-isnan": 2,
        "radix": 2,
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "space-infix-ops": 0,
        "valid-typeof": 2,
        "wrap-iife": [
            2,
            "outside"
        ],
        "strict": 0
    }
}